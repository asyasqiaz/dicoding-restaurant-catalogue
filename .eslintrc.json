{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "no-underscore-dangle": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "no-new": 0,
    "no-return-assign": 0,
    "no-prototype-builtins": 0,
    "no-alert": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0
  }
}
